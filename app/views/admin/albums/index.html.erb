<h1>List of Albums</h1>
<table>
    <tr>
        <thead>
            <th>Title</th>
            <th>Rating</th>
        </thead>
    </tr>
    <%@albums.each do |album| %>
    <tr>
        <td><%= albums.name%></td>
        <td><%= albums.age%></td>
        <td>
            <%= link_to "Details", admin_album_path(album) %>
            <%= link_to "Update"%>
            <%= link_to "Delete"%>
        </td>
    </tr>
    <%end%>
</table>


